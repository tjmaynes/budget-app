{
  "name": "budget-app",
  "description": "a simple web application for creating a financial budget",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "npm run build && npx http-server dist/ --port=3000",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
